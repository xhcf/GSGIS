/* 排行 */
.c_rank .panel-body{background:none;color:#fff;}
.c_rank .datagrid-header,.c_rank .datagrid-td-rownumber{background-color:transparent;}
.c_rank .datagrid-htable{background:none;color:#fff;}
.c_rank .datagrid-btable,.c_rank .datagrid-ftable{color:#fff;}
.c_rank .datagrid-body td,.c_rank .datagrid-footer td {
    border-color: #014A94;border-style: solid;
}
.c_rank .datagrid-header td {
    border-color: #014A94;border-style: solid;
}

.c_rank .datagrid-header-row{ border-top:1px solid #014A94;}
.c_rank .datagrid-cell u{color:#FFD000;}
.c_rank .datagrid-header td:last-child,.c_rank .datagrid-body td:last-child,.c_rank .datagrid-footer td:last-child{border-right-width: 0;}
.c_rank .datagrid-row-over{background:#000a7c;color: inherit;}
datagrid-row-selected{background:#000a7c;color: inherit;}

/* 地图图例 */
.searches li:before {
    content: "";
    position: absolute;
    top: 10px;
    left: 0;
    display: block;
    width: 16px;
    height: 10px;
    z-index: 8;
}
.searches li:before {
    background: #fff;
}
.searches ul li {
    float: left;
    width: 33%;
    line-height: 28px;
    height: 28px;
    vertical-align: middle;
    position: relative;
    padding-left: 17px;
}

.searches input[type="checkbox"] {
    visibility: hidden;
    width: 17px;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 10;
}
.searches ul li {
    height: 25px;
    line-height: 25px;
}
/*1366与1920的适配  2019年7月26日17:33:36  shihaiyan*/
@media screen and (max-width:1366px) {
    .c_view_top dt{padding-top:20% }
    c_view_top{padding-top: 0px;}
}
@media screen and (min-width:1367px) and (max-width:1919px) {
    .c_view_top dt{padding-top: 5% }
    c_view_top{padding-top: 2%;}
}
@media screen and (min-width:1920px) {
    .c_view_top dt{padding-top: 0% }
    c_view_top{padding-top: 4%;}
}
