<!--获取系统时间 -->
<e:q4o var="sys_date">
	SELECT TO_CHAR(SYSDATE,'YYYYMMDD') DAY_VAL,TO_CHAR(SYSDATE,'YYYY-MM') MONTH_VAL FROM DUAL
</e:q4o>
<e:set var="ACCT_DAY">
	${sys_date.DAY_VAL}
</e:set>
<e:set var="ACCT_MONTH">
	${sys_date.MONTH_VAL}
</e:set>
<html>
