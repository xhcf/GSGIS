	var url1 = 'http://135.149.64.144:9000/gsch-web/mkt/getBindAccount';
	var cust_name = "";
	var cust_addr = "";
	var contact_num = "";
	var acc_nbr = "";
	var scene_id = "";
	var crm_bind_win = "";
	var crm_id = "";

	function call_execute(element_id){
		$("#crm_"+element_id).click();
	}
	parent.call_execute_new_cust_view = call_execute;
	//crm跳转
	function execute_crm(id,cust_name1,cust_addr1,contact_num1,acc_nbr1,scene_id1){
		cust_name = cust_name1;
		cust_addr = cust_addr1;
		contact_num = contact_num1;
		acc_nbr = acc_nbr1;
		scene_id = scene_id1;
		if(id=="" || id==undefined){
			layer.msg("没有查到对应的划小工号，请等待同步后使用");
			return;
		}
		//调用划小接口检测当前账号是否绑定成功，如果成功进crm。否则弹出填写crm工号的页面进行绑定

		/*if(test!=undefined){
		 var cust_name2 = encodeURI(encodeURI(cust_name));//UTFTranslate.Change(cust_name);
		 var address2 = encodeURI(encodeURI(cust_addr));//UTFTranslate.Change(cust_addr);
		 var accnbr2 = encodeURI(encodeURI(contact_num));//UTFTranslate.Change(contact_num);
		 var broadband2 = encodeURI(encodeURI(acc_nbr));//UTFTranslate.Change(acc_nbr);
		 var crm_id = '316666';
		 //scene_id ren是续约 sin是单转融 2
		 var crm_url = 'http://10.52.66.110:8090/MmtWeb/home/verplatform.jsp?username=316666&userpwd=8763&code=8763&reqparam=';
		 //var crm_url = 'http://135.149.47.21/MmtWeb/home/verplatform.jsp?username=316666&userpwd=8763&code=8763&reqparam=';
		 //var crm_url = 'http://135.149.47.21/MmtWeb/home/verplatform.jsp?reqparam=';
		 $.post('<e:url value="crmUrl.e"/>',{"user_id":crm_id,"cust_name":cust_name,"address":cust_addr,"accnbr":contact_num,"broadband":acc_nbr,"bus_type":scene_id},function(data){
		 var reqparam = $.trim(data);//crm_id正式环境把316666换成crm_id
		 //弹窗被阻挡，window.open的写法不行，改为下面写法
		 //var tempwindow=window.open('_blank');
		 //tempwindow.location=crm_url+reqparam+"&cust_name="+cust_name2+"&address="+address2+"&accnbr="+accnbr2+"&broadband="+broadband2+"&bus_type="+scene_id;
		 console.log(cust_name2,address2,accnbr2,broadband2,scene_id);
		 //window.open(crm_url+reqparam+"&cust_name="+cust_name2+"&address="+address2+"&accnbr="+accnbr2+"&broadband="+broadband2+"&bus_type="+scene_id,'CRM预受理');
		 window.open(crm_url+reqparam,'CRM预受理');
		 });
		 return;
		 }*/
		$.post(url1,{"staffNo":id},function(data){
			var d1 = data;
			//var d1 = {"errorCode":0,"entity":{"STAFF_CODE":'',"MOBILE_PHONE":''}};
			if(d1.errorCode!='0'){//调用划小接口失败，
				layer.msg(d1.message);
				return;
			}else{
				var entity = d1.entity;
				if(entity.STAFF_CODE=="" || entity.STAFF_CODE==undefined){
					layer.msg("还未绑定CRM工号，请绑定");
					crm_bind_win = layer.open({
						title: '营销派单转CRM受理',
						type: 2,
						shade: 0,
						area: ["480px", "400px"],
						content: '<e:url value="/pages/telecom_Index/sub_grid/crm_bind_page.jsp" />?staff_no='+id,
						skin: 'sub_summary_div',
						cancel: function (index) {
						}
					});
				}else if(entity.MOBILE_PHONE=="" || entity.MOBILE_PHONE==undefined){//未绑定手机号
					layer.msg("没有在CRM绑定手机号，请先在CRM完善信息");
					return;
				}else{//已绑定成功
					crm_id = entity.STAFF_CODE;
					bound_ready_jump_win = layer.open({
						title: '营销派单转CRM受理',
						type: 2,
						shade: 0,
						area: ["480px", "200px"],
						content: "<e:url value='/pages/telecom_Index/sub_grid/crm_jump_page.jsp' />?staff_no="+id,
						skin: 'sub_summary_div',
						cancel: function (index) {
						}
					});
				}
			}
		});

	}

	function closeCrmBindWin(){
		layer.close(crm_bind_win);
	}
	function closeCrmJumpWin(){
		layer.close(bound_ready_jump_win);
	}

	function jumpToCRM(){
		var cust_name2 = encodeURI(encodeURI(cust_name));//UTFTranslate.Change(cust_name);
		var address2 = encodeURI(encodeURI(cust_addr));//UTFTranslate.Change(cust_addr);
		var accnbr2 = encodeURI(encodeURI(contact_num));//UTFTranslate.Change(contact_num);
		var broadband2 = encodeURI(encodeURI(acc_nbr));//UTFTranslate.Change(acc_nbr);
		var crm_id = crm_id;
		//scene_id ren是续约 sin是单转融  2018年6月22日，参数修改为相反
		//var crm_url = 'http://10.52.66.110:8090/MmtWeb/home/verplatform.jsp?username=316666&userpwd=8763&code=8763&reqparam=';
		//var crm_url = 'http://135.149.47.21/MmtWeb/home/verplatform.jsp?reqparam=';
		var crm_url = 'http://135.149.47.21/MmtWeb/home/verplatform.jsp?reqparam=';
		$.post('<e:url value="crmUrl.e"/>',{"user_id":crm_id,"cust_name":cust_name2,"address":address2,"accnbr":accnbr2,"broadband":broadband2,"bus_type":scene_id},function(data1){
			var reqparam = $.trim(data);//crm_id正式环境把316666换成crm_id
			/*layer.open({
			 title: "业务受理",
			 //title:false,
			 type: 2,
			 shade: 0,
			 area: ['100%','90%'],//sub_summary_div_size
			 content: crm_url+reqparam+"&cust_name="+cust_name+"&address="+address+"&accnbr="+accnbr+"&broadband="+broadband+"&bus_type="+bus_type,
			 skin: 'win_blue',
			 cancel: function (index) {
			 }
			 });*/
			//弹窗被阻挡，window.open的写法不行，改为下面写法
			//var tempwindow=window.open('_blank');
			//tempwindow.location=crm_url+reqparam+"&cust_name="+cust_name2+"&address="+address2+"&accnbr="+accnbr2+"&broadband="+broadband2+"&bus_type="+scene_id;
			window.open(crm_url+reqparam,'CRM预受理');
			//window.open(crm_url+reqparam+"&cust_name="+cust_name2+"&address="+address2+"&accnbr="+accnbr2+"&broadband="+broadband2+"&bus_type="+scene_id,'CRM预受理');
		});
	}

	$(function () {
		//支局名赋值
		$("#sub_name").text(sub_name);
		//标签页切换事件
		$("#jz_yw > span").each(function (index) {
			$(this).on("click", function () {
				tabSelectedReset();
				$(this).addClass("active").siblings().removeClass("active");
				$("#jz_marketing").children().not($(".div_" + index)).hide();
				$(".div_" + index).show();
				if (index == 0) {
					/* load_jz(); */
					//day_sence_all();
					//mon_sence_all();
					load_day_data("");
				} else if (index == 1) {
					load_yw();
				}
			});
		});

		$("#tab > span").each(function (index) {
			$(this).on("click", function () {
				tabSelectedReset();
				$(this).addClass("active").siblings().removeClass("active");
				$("#content1 > div").hide();
				$(".div_0_" + index).show();
				if (index == 0) {
					//load_tb_d(0);
					//load_dzr();
					//load_dzr_m();
					//模拟点击考勤统计-趋势日
					$("#third_tab > span").eq(0).click();
					//数据加载
					//load_day_data("");
				} else if (index == 1) {
					intraday_sence_id = '';
					load_intraday_data(intraday_sence_id);
				} else if (index == 2) {
					load_track_data(1);
				} else if (index == 3) {
					var succ_flag = 1;
					load_succ_data(succ_flag);
				}
			})

			//默认加载 精准营销 -->考核统计-->单转融
			/*  if (tab_index2 == "1" && index==1){
			 $(this).click();
			 }else{
			 load_dzr();
			 load_dzr_m();
			 }  */
		})

		//单转融   合约到期切换
		//日表
		$(".summary_day_head>p>span").each(function (index) {
			$(this).unbind();
			$(this).on("click", function () {
				tabSelectedReset();
				$(this).addClass("active").siblings().removeClass("active");

				//显示当前表格，隐藏其他表格
				if (index == 0) {
					day_sence_all();
				} else if (index == 1) {
					load_dzr();
				}else if (index ==2) {
					load_hydq();
				}if(index ==3) {
					load_cmhx();
				}
			})
		})

		//月表
		$(".summary_month_head2>p>span").each(function (index) {
			$(this).unbind();
			$(this).on("click", function () {
				tabSelectedReset();
				$(this).addClass("active").siblings().removeClass("active");
				//显示当前表格，隐藏其他表格
				if (index == 0) {
					mon_sence_all();
				} else if (index == 1) {
					load_dzr_m();
				}else if (index == 2) {
					load_hydq_m();
				}if(index ==3) {
					load_cmhx_m();
				}
			})
		})

		//有意向、同意办理……等页签切换
		var flag1=1;
		$("#track_tb > span").each(function (index) {
			$(this).unbind();
			$(this).on("click", function () {
				tabSelectedReset();
				$(this).addClass("active").siblings().removeClass("active");
				//显示当前表格，隐藏其他表格

				if(index==0){
					flag1=1;
				}else if(index==1){
					flag1=2;
				}else if(index==2){
					flag1=3;
				}else if(index==3){
					flag1=4;
				}
				load_track_data(flag1);
			})
		})

		//考核统计---趋势日 网格(日)网格(月)……等页签切换
		$("#third_tab > span").each(function (index) {
			$(this).unbind();
			$(this).on("click", function () {
				tabSelectedReset();
				$(this).addClass("active").siblings().removeClass("active");
				//显示当前表格，隐藏其他表格
				if(index==0){
					$("#khtj_qsr").show();
					$("#khtj_wgr").hide();
					$("#khtj_wgy").hide();
					//load_tb_d(flag);

					load_day_data("");
				}else if(index==1){//网格(日)
					$("#khtj_qsr").hide();
					$("#khtj_wgr").show();
					$("#khtj_wgy").hide();
					//load_tb_m(flag);
					//load_tb_wgd("");
					load_wgd_data("");
				}else if(index==2){//网格(月)
					$("#khtj_qsr").hide();
					$("#khtj_wgr").hide();
					$("#khtj_wgy").show();
					load_wgm_data("");
				}
				//数据加载

			})
		})
		//网格日 场景切换
		$("#summary_day_left_1 > span").each(function (index) {
			$(this).unbind();
			$(this).on("click", function () {
				tabSelectedReset();
				$(this).addClass("active").siblings().removeClass("active");
				//显示当前表格，隐藏其他表格
				var wg_day_index= ''
				//显示当前表格，隐藏其他表格
				if(index==0){
					wg_day_index='';
				}else if(index==1){
					wg_day_index='10';
				}else if(index==2){
					wg_day_index='11';
				}else if(index==3){
					wg_day_index='12';
				}

				load_wgd_data(wg_day_index);
			})
		})

		//网格月 场景切换
		$("#summary_day_left_2 > span").each(function (index) {
			$(this).unbind();
			$(this).on("click", function () {
				tabSelectedReset();
				$(this).addClass("active").siblings().removeClass("active");
				//显示当前表格，隐藏其他表格
				var wg_month_index= ''
				//显示当前表格，隐藏其他表格
				if(index==0){
					wg_month_index='';
				}else if(index==1){
					wg_month_index='10';
				}else if(index==2){
					wg_month_index='11';
				}else if(index==3){
					wg_month_index='12';
				}

				load_wgm_data(wg_month_index);
			})
		})

		//设置滚动事件
		$(".dispatch_m_tab").scroll(function () {
			var viewH = $(this).height();
			var contentH = $(this).get(0).scrollHeight;
			var scrollTop = $(this).scrollTop();
			if (scrollTop / (contentH - viewH) >= 0.95) {
				if (new Date().getTime() - begin_scroll > 500) {
					var params = {
						eaction: "marketing_intraday",
						page: ++list_page,
						flag: '5',
						union_org_code: substation,
						intraday_sence_id: intraday_sence_id
						//last_month:'${last_month.val}'
					}
					intradayListScroll(params, 0);
				}
				begin_scroll = new Date().getTime();
			}
		});

		//设置滚动事件
		$(".follow_m_tab").scroll(function () {
			console.log(1);
			var viewH = $(this).height();
			var contentH = $(this).get(0).scrollHeight;
			var scrollTop = $(this).scrollTop();
			if (scrollTop / (contentH - viewH) >= 0.95) {
				if (new Date().getTime() - begin_scroll > 500) {
					var params = {
						eaction: "marketing_track",
						page: ++list_page,
						flag:'5',
						union_org_code:substation,
						exec_stat:flag1
						//last_month:'${last_month.val}'
					}
					trackListScroll(params, 0);
				}
				begin_scroll = new Date().getTime();
			}
		});

		//设置滚动事件
		$(".succ_info_list").scroll(function () {
			var viewH = $(this).height();
			var contentH = $(this).get(0).scrollHeight;
			var scrollTop = $(this).scrollTop();
			if (scrollTop / (contentH - viewH) >= 0.95) {
				if (new Date().getTime() - begin_scroll > 500) {
					var params = {
						eaction: "marketing_succ",
						page: ++list_page,
						flag:'5',
						union_org_code:substation,
						//last_month:'${last_month.val}'
					}
					succListScroll(params,1);
				}
				begin_scroll = new Date().getTime();
			}
		});

		$(".warning_m_tab").scroll(function () {
			var viewH = $(this).height();
			var contentH = $(this).get(0).scrollHeight;
			var scrollTop = $(this).scrollTop();
			if (scrollTop / (contentH - viewH) >= 0.95) {
				if (new Date().getTime() - begin_scroll > 500) {
					var params = {
						eaction: "marketing_warning",
						page: ++list_page,
						substation: substation
					}
					warningListScroll(params, 0);
				}
				begin_scroll = new Date().getTime();
			}
		});

		//默认先加载第一个标签的数据
		//load_summary();
		var tab_index = "${param.tab_index}";
		if (tab_index == 0) {
			$("#jz_yw span").eq(0).click();
		} else if (tab_index == 1) {
			$("#jz_yw span").eq(1).click();
		} else {
			$("#jz_yw span").eq(0).click();
		}

		$("#CRM_jump").on("click",function(){
			jumpToCRM();
		});
	});
</script>
