html, body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, pre, form, fieldset, p, label, table, th, tr, td, object,select, input{ margin: 0px; padding: 0px; }
body{ font-size: 12px;font-family: arial,"微软雅黑";color: #434343;font-weight: normal;letter-spacing: 0.25px; }
fieldset, img, a img, a:link img, a:visited img { border: 0px; }
table{ border-collapse:collapse;}
address, caption, cite, code, dfn, th, var { font-style: normal; font-weight: normal; }
ol, ul { list-style: none; }
h1, h2, h4, h5 { font-size: 100%; vertical-align: central; }
a,button,input,textarea{-webkit-tap-highlight-color: rgba(0,0,0,0);}/*去掉用户点击一个元素时出现的半透明灰色遮罩*/
a{ font-size:12px; text-decoration:none;}
h1,h,h3,h4,h5{font-weight:normal}
.clearfix{overflow:hidden;_zoom:1; clear:both;}
#content:after{content:".";display:block;clear:both;visibility:hidden;height:0}
.fl{ float:left}
.fr{ float:right}
a {text-decoration:none;}


/*渠道画像*/
.portrait { width:100%; background: #fff;margin: 0 auto;}
.p_title{width: 100%;height: 50px;background:#0212A1 ;}
.p_title p{width: 40%;margin: 0px auto;text-align: center; background: url(../images/p_title.png) no-repeat center top;height: 50px;color: #fff;font-size: 22px;font-weight: bold;line-height: 40px;}
.p_tab{width: 100%;height: 26px; background:#0066ff;}
.p_tab li{font-weight: bold; border-right: 1px solid #8A90C9; color: #fff;font-size: 16px; padding: 0 20px;line-height: 18px;float: left; cursor: pointer;}
