.basic_top i{display: inline-block;vertical-align: middle; width: 26px;height: 26px; background: url(../images/basic_title.png) no-repeat;margin-right: 10px; }
.tag{float: right; text-align: right;    margin-top: 2px;}
.tag span{border: 1px solid #FF6600; color: #FF6600; border-radius: 2px; padding: 2px 10px; font-size: 12px; font-weight: normal;}
.basic_com{margin-bottom: 20px; border:1px solid #DCDCDC; border-radius: 4px;padding:10px 10px 15px 10px; position: relative;}
.basic_info{color: #0059B2; font-weight: bold; font-size: 15px; margin-bottom: 10px; position: absolute; top:-10px; background: #fff; padding: 0 10px;}
