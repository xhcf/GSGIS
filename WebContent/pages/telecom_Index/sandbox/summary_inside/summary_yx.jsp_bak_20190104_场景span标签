		params.union_org_code=substation;
		params.acct_day=selectDay;
		var postUrl="<e:url value='pages/telecom_Index/common/sql/tabData_sandbox_summary_inside_yx.jsp?eaction=marketing_sence_wgday_num'/>";
		$.post(postUrl,params, function (data) {
			data = $.parseJSON(data);
			if(data.length>0){
				//全部
				/*$("#wg_sence_all_day").html("("+((data[0].SUM_NUM==null)?0:data[0].SUM_NUM)+")");
				 //单转融
				 $("#wg_dzr_day").html("("+((data[0].DR_NUM==null)?0:data[0].DR_NUM)+")");
				 //协议到期
				 $("#wg_xydq_day").html("("+((data[0].XY_NUM==null)?0:data[0].XY_NUM)+")");
				 //沉默唤醒
				 $("#wg_cmhx_day").html("("+((data[0].CMHX_NUM==null)?0:data[0].CMHX_NUM)+")");*/
				$("#wg_sence_all_day").html("("+data[0].MKT_CNT+")");
				$("#wg_dzr_day").html("("+data[1].MKT_CNT+")");
				$("#wg_xydq_day").html("("+data[2].MKT_CNT+")");
				$("#wg_cmhx_day").html("("+data[3].MKT_CNT+")");
			}
		})
	}
	/*-考核统计 end-*/
	/*-当日派单 start-*/
	//滚动加载 当日派单
	// 绑定事件
	var intraday_sence_id='';
	$(function(){
		$("#intraday_tb >span ").each(function (index) {
			$(this).unbind();
			$(this).on("click", function () {
				tabSelectedReset();
				$(this).addClass("active").siblings().removeClass("active");
				intraday_sence_id='';
				//显示当前表格，隐藏其他表格
				if(index==0){
					intraday_sence_id='';
				}else if(index==1){
					intraday_sence_id='10';
				}else if(index==2){
					intraday_sence_id='11';
				}else if(index==3){
					intraday_sence_id='12';
				}
				load_intraday_data(intraday_sence_id);
			})
		})

		//时间控件初始化
		initDateSelect();
	});
	var url = "<e:url value='/pages/telecom_Index/common/sql/tabData_sandbox_summary_inside.jsp'/>";
	var load_intraday_data = function (intraday_sence_id){
		intraday_num();
		var params = {
			eaction: "marketing_intraday",
			page: 0,
			union_org_code:substation,
			intraday_sence_id:intraday_sence_id
		}
		clear_data();
		intradayListScroll(params,1);
	};
	yx_load_intraday_data_view = load_intraday_data;

	//加载表格数据

	function intradayListScroll(params, flag) {
		var $grid_list = $("#dispatch_info_list");
		$.post(url, params, function (data) {
			data = $.parseJSON(data);
			for (var i = 0, l = data.length; i < l; i++) {
				var d = data[i];
				var newRow = "<tr><td style='width:35'>" + (++seq_num) + "</td>";
				newRow += "<td style='width:210'>" + d.CONTACT_ADDS + "</td><td style='width:90'><span>"+d.CONTACT_TEL +"</span></br>"+ d.SERV_NAME + "</td><td style='width:90'><a onclick=\"exec_agent('" + d.PROD_INST_ID + "','" + d.ORDER_ID + "','"+d.ADDRESS_ID+"','" + d.SEGM_ID +"',0,1,'"+ d.SCENE_ID_NUM+"','','1')\">" + d.ACC_NBR
				+ "</a></td><td style='width:210;text-align: left;'><span>【" + d.SCENE_ID+"】</span></br>"+d.MKT_REASON+"</td><td style='width:35'><a onclick=\"exec_agent('" + d.PROD_INST_ID + "','" + d.ORDER_ID + "','"+d.ADDRESS_ID+"','" + d.SEGM_ID +"',2,1,'"+ d.SCENE_ID_NUM+"','','1')\">执行</a>";
				if(d.SCENE_ID_NUM=='10' || d.SCENE_ID_NUM=='11')
					newRow += "<a style='display:none;' id=\"crm_"+d.PROD_INST_ID+"\" onclick=\"execute_crm('${sessionScope.UserInfo.EXT30}','"+d.SERV_NAME+"','"+ d.CONTACT_ADDS+"','"+ d.CONTACT_TEL +"','"+ d.ACC_NBR +"','"+ d.SCENE_ID_CRM+"')\">受理</a>";
				newRow += "</td></tr>";
				$grid_list.append(newRow);
			}
			if (data.length == 0 && flag) {
				$grid_list.empty();
				$grid_list.append("<tr><td style='text-align:center' colspan=6 onMouseOver=\"this.style.background='rgb(250,250,250)'\">没有查询到数据</td></tr>")
				return;
			}
		});
	}

	//执行，调用父页面的客户视图
	function exec_agent(prod_inst_id,order_id,add6,add4,tab_id,exec_able,scene_id,is_lost,is_yx){
		var param = {};
		param.prod_inst_id = prod_inst_id;
		param.order_id = order_id;
		param.add6 = add6;
		param.add4 = add4;
		param.tab_id = tab_id;
		param.exec_able = exec_able;
		param.scene_id = scene_id;
		param.is_lost = is_lost;
		param.is_yx = is_yx;
		openCustViewByProdInstId(param);
	}

	//当日派单场景记录数
	function intraday_num(){
		var params = {};
		params.union_org_code=substation;
		var postUrl="<e:url value='pages/telecom_Index/common/sql/tabData_sandbox_summary_inside.jsp?eaction=intraday_num'/>";
		$.post(postUrl,params, function (data) {
			data = $.parseJSON(data);
			if(data.length>0){
				$("#intraday_all").html("("+data[0].MKT_CNT+")");
				$("#intraday_dzr").html("("+data[1].MKT_CNT+")");
				$("#intraday_xydq").html("("+data[2].MKT_CNT+")");
				$("#intraday_cmhx").html("("+data[3].MKT_CNT+")");
			}
		})
	}
	/*-当日派单 end-*/

	/*-跟踪用户 start-*/
	function load_track_data(flag1){
		track_mun();
		var params = {
			eaction: "marketing_track",
			page: 0,
			union_org_code:substation,
			exec_stat:flag1
		}
		clear_data();
		trackListScroll(params,1);
	};

	//加载表格数据
	function trackListScroll(params,flag) {
		var $grid_list = $("#follow_info_list");
		$.post(url,params, function (data) {
			data = $.parseJSON(data);
			for (var i = 0, l = data.length; i < l; i++) {
				var d = data[i];
				var newRow = "<tr><td>" + (++seq_num) + "</td>";
				//newRow += "<td style='width:120px'><a href=\"javascript:void(0);\" onclick=\"standard_position_load('" + d.GRID_NAME + "',this)\" >" + d.GRID_NAME + "</a></td>";
				newRow += "<td style='text-align:left'>" + d.CONTACT_ADDS + "</td><td><span>"
				+"<a onclick=\"exec_agent('" + d.PROD_INST_ID + "','" + d.ORDER_ID + "','"+d.ADDRESS_ID+"','" + d.SEGM_ID +"',0,1,'"+ d.SCENE_ID+"','','1')\">"+ d.ACC_NBR+"</a>";
				if(d.SCENE_ID_NUM=='10' || d.SCENE_ID_NUM=='11')
					newRow += "<a style='display:none;' id=\"crm_"+d.PROD_INST_ID+"\" onclick=\"execute_crm('${sessionScope.UserInfo.EXT30}','"+d.SERV_NAME+"','"+ d.CONTACT_ADDS+"','"+ d.CONTACT_TEL +"','"+ d.ACC_NBR +"','"+ d.SCENE_ID_CRM+"')\">受理</a>";
				newRow += "</span></br>"+d.SERV_NAME+
				"</td><td style='text-align:left'><span>【" +d.MKT_CONTENT + "】</span></br>"+d.MKT_REASON +   "</td><td >"
				+ d.EXEC_TIME + "</td><td >"
				+ d.EXEC_DESC + "</td></tr>";
				$grid_list.append(newRow);
			}
			if (data.length == 0 && flag) {
				$grid_list.empty();
				$grid_list.append("<tr><td style='text-align:center' colspan=6 onMouseOver=\"this.style.background='rgb(250,250,250)'\">没有查询到数据</td></tr>")
				return;
			}
		});
	}

	//获取根据用户—>有意向、同意办理等记录条数
	function track_mun(){
		var params = {};
		params.union_org_code=substation;
		var postUrl="<e:url value='pages/telecom_Index/common/sql/tabData_sandbox_summary_inside.jsp?eaction=track_mun'/>";
		$.post(postUrl,params, function (data) {
			data = $.parseJSON(data);
			if(data.length>0){
				$("#track_mybe").html("("+data[0].EXEC1+")");
				$("#track_agree").html("("+data[0].EXEC2+")");
				$("#track_refuse").html("("+data[0].EXEC3+")");
				$("#track_uncontact").html("("+data[0].EXEC4+")");
			}
		})
	}
	/*-跟踪用户 end-*/
	/*-成功用户 start-*/
	function load_succ_data(succ_flag){
		succ_num();
		var params = {
			eaction: "marketing_succ",
			page: 0,
			union_org_code:substation,
			succ_flag:succ_flag
		}
		clear_data();
		succListScroll(params,1);
	};

	//加载表格数据
	function succListScroll(params, flag) {
		var $grid_list = $("#succ_info_list");
		$.post(url, params, function (data) {
			data = $.parseJSON(data);
			var seq_num = 0;
			for (var i = 0, l = data.length; i < l; i++) {
				var d = data[i];
				var newRow = "<tr><td>" + (++seq_num) + "</td>";
				//newRow += "<td style='width:120px'><a href=\"javascript:void(0);\" onclick=\"standard_position_load('" + d.GRID_NAME + "',this)\" >" + d.GRID_NAME + "</a></td>";
				newRow += "<td>" +  d.ACC_NBR +
					//"</td><td>" + d.CONTACT_CONTPERSON +
				"</td><td>" + d.SERV_NAME +
				"</td><td style='text-align:left'><span>【" + d.MKT_CONTENT +"】</span></br>"+d.MKT_REASON+
				"</td><td>" + d.SUCC_BUSINESS +
				"</td><td>" +  d.SUCC_TIME +
				"</td></tr>";
				$grid_list.append(newRow);
			}
			if (data.length == 0 && flag) {
				$grid_list.empty();
				$grid_list.append("<tr><td style='text-align:center' colspan=6 onMouseOver=\"this.style.background='rgb(250,250,250)'\">没有查询到数据</td></tr>")
				return;
			}
		});
	}


	//获取成功用户数
	function succ_num() {
		var params = {};
		params.union_org_code = substation;
		var postUrl = "<e:url value='pages/telecom_Index/common/sql/tabData_sandbox_summary_inside.jsp?eaction=succ_num'/>";
		$.post(postUrl, params, function (data) {
			data = $.parseJSON(data);
			if (data.length > 0) {
				$("#succ_val").html(data[0].NUM);
			}
		})
	}
	/*-成功用户 end-*/

	//异网营销 异网预警
	function load_yw() {
		$("#marketing_warning_info_list").empty();
		var params = {
			eaction: "marketing_warning",
			page: 0,
			substation: substation
		}
		clear_data();
		warningListScroll(params, 1);
		params.eaction = "marketing_warning_count";
		$.post(url, params, function (data) {
			if (data != null && data.trim() != 'null') {
				data = $.parseJSON(data);
				$("#marketing_warning_count").html( data[0].C_NUM );
			} else {
				$("#marketing_warning_count").html("(0)");
			}
		})
	}

	function warningListScroll(params, flag) {
		var $grid_list = $("#marketing_warning_info_list");
		$.post(url, params, function (data) {
			data = $.parseJSON(data);

			for (var i = 0, l = data.length; i < l; i++) {
				var d = data[i];
				var newRow = "<tr><td style='width: 40px'>" + (++seq_num) + "</td>";
				newRow += "<td style='width: 50px'>" + d.CONTACT_PERSON + "</td><td style='width: 200px'>" + d.STAND_NAME_2 +
				"</td><td style='width: 50px'>" + d.KD_BUSINESS +"</td><td style='width:100px'>" + d.CONTACT_NBR + "</td><td style='width:80px'>"+ d.KD_DQ_DATE + "</td>";
				//newRow += "<td style='width: 50px'><a href='javascript:void(0);' onclick='execute(" + d.SEGM_ID_2 + ")'>执行</a></td></tr>"
				newRow += "<td style='width: 50px'><a href='javascript:void(0);' onclick='execute(" + d.SEGM_ID_2 + ")'></a></td></tr>"
				$grid_list.append(newRow);
			}
			if (data.length == 0 && flag) {
				$grid_list.empty();
				$grid_list.append("<tr><td style='text-align:center' colspan=9 onMouseOver=\"this.style.background='rgb(250,250,250)'\">没有查询到数据</td></tr>")
				return;
			}
		});
	}

	var UTFTranslate = {
		Change:function(pValue){
			return pValue.replace(/[^\u0000-\u00FF]/g,function($0){return escape($0).replace(/(%u)(\w{4})/gi,"&#x$2;")});
		},
		ReChange:function(pValue){
			return unescape(pValue.replace(/&#x/g,'%u').replace(/\\u/g,'%u').replace(/;/g,''));
		}
	};

